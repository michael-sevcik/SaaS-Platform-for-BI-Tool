{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ManagementApp__FrontendUrl": "http://localhost:5127",
        "ManagementApp__DefaultAdminEmail": "admin@admin.cz"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5127"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ManagementApp__FrontendUrl": "https://localhost:7268",
        "ManagementApp__DefaultAdminEmail": "admin@admin.cz"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7268;http://localhost:5127"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "ManagementApp__FrontendUrl": "http://localhost:8080",
        "ManagementApp__DefaultAdminEmail": "admin@admin.cz"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:55183",
      "sslPort": 44358
    }
  }
}